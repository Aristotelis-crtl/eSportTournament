@page
@model eSportTournament.Pages.IndexCompetModel

@{
    ViewData["Title"] = "IndexCompet";
}

<h1>IndexCompet</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionEquipe[0].CompetitionCs)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionEquipe[0].EquipeE)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.CompetitionEquipe) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitionCs.nomCompetition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EquipeE.nomEquipe)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
